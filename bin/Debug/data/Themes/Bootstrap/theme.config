$style:css\bootstrap.min.css



@module
<div class='module'>
	<h3 class='module-name'>[module_name]</h3>
	<p class='module-info'>[module_info]</h3>
	<div class="classes"><h3>Classes</h3>
		[classes]
	</div>
	<div class="functions"><h3>Functions</h3>
		[functions]
	</div>
</div>


@class
<div class="class">
	<h3 class='class-name'>[class_name]</h3>
	<p class="module-info">[class_info]</p>
	<div class="methods"><h3>Methods</h3>
		[methods]
	</div>
</div>


@method
<div class="method">
	<h3 class="method-name">[method_name]<span>[method_info]</h3>
</div>
