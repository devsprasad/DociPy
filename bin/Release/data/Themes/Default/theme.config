$style:default.css
$style:index.css

@indexmodule
<div class="index-module">
	<h3 class='index-module-name'><a href="[module_path]">[module_name]</a></h3>
	<p class='index-module-info'>[module_info]</p>	
</div>

@indexclass
<div class="index-class">
	<h3 class='index-class-name'><a href="[module_path]#[class_name]">[class_name]</a></h3>
</div>


@module
<div class='module'>
	<h3 class='module-name'>[module_name]</h3>
	<p class='module-info'>[module_info]</p>
	<div class="classes"><h3 class="class-head">Classes</h3>
		[classes]
	</div>
	<div class="functions"><h3 class="function-head">Functions</h3>
		[functions]
	</div>
</div>

@class
<div class="class">
	<h3 class='class-name'>[class_name]</h3>
	<p class="module-info">[class_info]</p>
	<div><span class="label">Base Classes: </span>[bases]</div>
	<div class="methods"><h3 class='method-head'>Methods</h3>
		[methods]
	</div>
	<div class="properties"><h3 class='property-head'>Properties</h3>
		[properties]
	</div>
</div>

@parent_class
<span class="bg-primary">[base]</span>


@constructor
<div class="method">
	<h3 class="method-name">[method_name]<span>( [arg_tuple] )</span><span class="badge">constructor</span></h3><p class="method-info">[method_info]</p>
	<div class="args"><h4 class="args-head">Arguments</h4>
		[arguments]
	</div>
</div>

@destructor
<div class="method">
	<h3 class="method-name">[method_name]<span>( [arg_tuple] )</span><span class="badge">destructor</span></h3><p class="method-info">[method_info]</p>
	<div class="args"><h4 class="args-head">Arguments</h4>
		[arguments]
	</div>
</div>

@property
<div class="property">
	<h3 class="property-name">[property_name]<span class="badge [property_get]" >Readable</span><span class="badge [property_set]" >Writeable</span></span></h3>
</div>


@method
<div class="method">
	<h3 class="method-name">[method_name]<span>( [arg_tuple] )</span></h3><p class="method-info">[method_info]</p>
	<div class="args"><h4 class="args-head">Arguments</h4>
		[arguments]
	</div>
</div>


@function
<div class="function">
	<h3 class="function-name">[function_name]<span>( [arg_tuple] )</span></h3><p class="function-info">[function_info]</p>
	<div class="args"><h4 class="args-head">Arguments</h4>
		[arguments]
	</div>
</div>
