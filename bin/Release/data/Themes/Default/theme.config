	<h3 class="function-name">[function_name]<span>[function_info]</span></h3>
	<div class="args"><h4 class="args-head">Arguments</h4>
		[arguments]
	</div>
</div> -->

@args
<div class="arg">
	<h4 class="arg-name">[arg_name]</h4><p class="arg-info">[arg_info] (Default = [arg_default])</p>
</div>

@arg
<label class="arg">[arg_name] [arg_default]</label>


