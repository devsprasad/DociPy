$style:css\bootstrap.css

@module
<div class='module'>
	<h3 class='module-name'>[module_name]</h3>
	<p class='module-info'>[module_info]</p>
	<div class="classes"><h3>Classes</h3>
		[classes]
	</div>
	<div class="functions"><h3>Functions</h3>
		[functions]
	</div>
</div>


@class
<div class="class">
	<h3 class='class-name'>[class_name]</h3>
	<p class="module-info">[class_info]</p>
	<div class="methods"><h3>Methods</h3>
		[methods]
	</div>
</div>


@method
<div class="method">
	<h3 class="method-name">[method_name]<span>[method_info]</span></h3>
	<div class="args"><h4>Arguments</h4>
		[arguments]
	</div>
</div>


@function
<div class="function">
	<h3 class="function-name">[function_name]<span>[function_info]</span></h3>
	<div class="args"><h4>Arguments</h4>
		[arguments]
	</div>
</div>

@arg
<div class="arg">
	<h4 class="arg-name">[arg_name]<span>[arg_info] (Default = [arg_default])</span></h4>
</div>

