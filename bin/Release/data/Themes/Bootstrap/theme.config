$style:default.css


@module
<div class='module'>
	<h3 class='module-name'>[module_name]</h3>
	<p class='module-info'>[module_info]</p>
	<div class="classes"><h3 class="class-head">Classes</h3>
		[classes]
	</div>
	<div class="functions"><h3 class="function-head">Functions</h3>
		[functions]
	</div>
</div>


@class
<div class="class">
	<h3 class='class-name'>[class_name]</h3>
	<p class="module-info">[class_info]</p>
	<div class="methods"><h3 class='method-head'>Methods</h3>
		[methods]
	</div>
</div>


@method
<div class="method">
	<h3 class="method-name">[method_name]<span>[arg_tuple]</h3><p class="method-info">[method_info]</p>
	<div class="args"><h4 class="args-head">Arguments</h4>
		[arguments]
	</div>
</div>


@function
<div class="function">
	<h3 class="function-name">[function_name]<span>[function_info]</span></h3>
	<div class="args"><h4 class="args-head">Arguments</h4>
		[arguments]
	</div>
</div>

@args
<div class="arg">
	<h4 class="arg-name">[arg_name]<span>[arg_info] (Default = [arg_default])</span></h4>
</div>

@arg
<label class="arg">[arg_name] [arg_default]</label>


